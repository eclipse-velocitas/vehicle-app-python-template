{
    "manifestVersion": "v3",
    "name": "SampleApp",
    "interfaces": [
        {
            "type": "vehicle-signal-interface",
            "config": {
                "src": "https://github.com/COVESA/vehicle_signal_specification/releases/download/v4.0/vss_rel_4.0.json",
                "datapoints": {
                    "required": [
                        {
                            "path": "Vehicle.Speed",
                            "access": "read"
                        }
                    ]
                }
            }
        },
        {
            "type": "pubsub",
            "config": {
                "reads": [
                    "sampleapp/getSpeed"
                ],
                "writes": [
                    "sampleapp/getSpeed/response",
                    "sampleapp/currentSpeed"
                ]
            }
        }
    ]
}
